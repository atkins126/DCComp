  rtl,
  fmx,
  IndySystem,
  IndyProtocols,
  IndyCore,
  dbrtl;

contains
  aOPCSource in '..\Source\Common\aOPCSource.pas',
  aOPCTCPSource in '..\Source\Common\aOPCTCPSource.pas',
  aOPCTCPSource_V30 in '..\Source\Common\aOPCTCPSource_V30.pas',
  aOPCTCPSource_V31 in '..\Source\Common\aOPCTCPSource_V31.pas',
  aOPCTCPSource_V32 in '..\Source\Common\aOPCTCPSource_V32.pas',
  aOPCLookupList in '..\Source\Common\aOPCLookupList.pas',
  aOPCLog in '..\Source\Common\aOPCLog.pas',
  aOPCTagDictionary in '..\Source\Common\aOPCTagDictionary.pas',
  aOPCProvider in '..\Source\Common\aOPCProvider.pas',
  uDCStrResource in '..\Source\Common\uDCStrResource.pas';

end.

